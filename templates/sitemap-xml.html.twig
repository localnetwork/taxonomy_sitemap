<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  {% for term in terms %}
    {% set url = url('entity.taxonomy_term.canonical', { 'taxonomy_term': term.id }) %}
    <url>
      <loc>{{ url['#markup'] }}</loc> 
      <lastmod>{{ term.terminfo.changed.value|date("c") }}</lastmod>
      <changefreq>Never</changefreq>
      <priority>{{ term.priority }}</priority>
    </url>
  {% endfor %}
</urlset>


